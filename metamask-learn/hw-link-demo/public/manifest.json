{
  "manifest_version": 3,
  "background": {
    "service_worker": "js/serviceWorker.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon16.png",
      "32": "icons/icon16.png"
    },
    "default_title": "1 - 硬件钱包链接测试",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/contentscript.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  }
}
